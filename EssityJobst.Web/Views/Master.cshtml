    <!--- Footer code here ---->
        <div class="footer">
            <div class="container">
                <div class="row">
                    @Html.Partial("Footer/JobSTInfo")
                    @Html.Partial("Footer/FooterCollection")
                    <div class="col-md-3 col-pd-0">
                        @Html.Partial("Footer/PaymentModeList")
                        @Html.Partial("Footer/SocialMediaCollection")
                    </div>
                </div>
                <hr>
                @Html.Partial("Footer/FooterImageDescription")
            </div>
        </div>
    
    
  </div>
      <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
