@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Country>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/Demo.css" />
    <title>@Model.Value("pageTitle")</title>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <!-- /.container -->
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    @*<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
    <script src="https://cdn.linearicons.com/free/1.0.0/svgembedder.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>

    <script>
        $(document).ready(function () {
            //alert('Hi');
            $.ajax({
                url: '/Umbraco/Surface/Cloud/GetUsers',
                type: 'GET',
                cache: false,
                dataType: 'json',
                success: function (result) {
                    alert('Success');
                    for (var i in result) {
                        $('#parent').append('<p>' + result[i].Name + ' ' + result[i].Family + '</p>');
                    }
                },
                error: function (response) {
                   // alert('eror');
                }
            });
            //$.ajax({
            //    type: "GET",
            //    url: "/Home/GetUsers",
            //    contentType: "application/json;charset=utf-8",
            //    dataType: "json",

            //});
        });


    </script>
</head>
<body>
@*Html.Partial("Header/Menu")*@
@RenderBody()

    <script type="text/jscript">
        var url = "/Umbraco/Surface/Cloud/TellMeDate";
        var url1 = "/Umbraco/Surface/Cloud/GetUsers";

        $.get(url, null, function (data) {
            $("#rData").html(data);
        });
        $.get(url1, null, function (data) {
            //$("#rData").html(data);
            for (var i in data) {
                $('#parent').append('<p>' + data[i].Name + '</p>');
            }
        });
    </script>

</body>
</html>
